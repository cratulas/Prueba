<p id="notice"><%= notice %></p>

<h1 class="text-center my-5">Types</h1>
<div class="container">

  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>ID</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  
    <tbody>
      <% @types.each do |type| %>
        <tr>
          <td><%= type.name %></td>
          <td><%= type.id %></td>
          <td><%= link_to 'Show', type %></td>
          <td><%= link_to 'Edit', edit_type_path(type) %></td>
          <td><%= link_to 'Destroy', type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  
  <br>
  
  <%= link_to 'New Type', new_type_path, class: "btn btn-primary" %>
  


</div>
